{
  "name": "barisdemirhan/useinbox",
  "description": "UseInbox Laravel Package",
  "keywords": [
    "barisdemirhan",
    "useinbox"
  ],
  "homepage": "https://github.com/barisdemirhan/useinbox",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Barış Demirhan",
      "email": "hello@barisdemirhan.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4",
    "guzzlehttp/guzzle": "^7.4",
    "illuminate/support": "^6.2"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Barisdemirhan\\Useinbox\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Barisdemirhan\\Useinbox\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "UseInboxServiceProvider"
      ],
      "aliases": {
        "Useinbox": "UseInboxFacade"
      }
    }
  }
}
